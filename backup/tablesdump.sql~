drop database if exists submission_service_db;
create database submission_service_db;
use submission_service_db;

DROP TABLE IF EXISTS code_list;
CREATE TABLE code_list (
  id_code_list varchar(25) NOT NULL,
  description varchar(250) DEFAULT NULL,
  PRIMARY KEY (id_code_list)
) ENGINE=InnoDB DEFAULT CHARSET=latin1;

DROP TABLE IF EXISTS execution;
CREATE TABLE execution (
  id_execution varchar(25) NOT NULL,
  description varchar(250) DEFAULT NULL,
  PRIMARY KEY (id_execution),
) ENGINE=InnoDB DEFAULT CHARSET=latin1;

